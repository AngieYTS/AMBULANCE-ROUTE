<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contenido"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Ambulance_Route"
    tools:showIn="@layout/app_bar_ambulance__route">


    <android.support.constraint.ConstraintLayout
        android:id="@+id/conten"
        android:layout_width="388dp"
        android:layout_height="518dp"
        android:background="@android:color/holo_blue_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="286dp"
            android:layout_height="301dp"
            android:layout_marginBottom="23dp"
            android:layout_marginEnd="51dp"
            android:layout_marginStart="51dp"
            app:layout_constraintBottom_toTopOf="@+id/btnsolicitar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/alerta" />

        <Button
            android:id="@+id/btnsolicitar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="101dp"
            android:layout_marginStart="101dp"
            android:layout_marginTop="23dp"
            android:text="Solicitar Ambulancia"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" />

        <Button
            android:id="@+id/btnNumerosEmergencia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="78dp"
            android:layout_marginEnd="93dp"
            android:layout_marginStart="94dp"
            android:layout_marginTop="20dp"
            android:text="Numeros de emergencia"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnsolicitar" />
    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>